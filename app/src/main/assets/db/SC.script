CREATE MEMORY TABLE PUBLIC.ORDER_PRODUCTS(OID VARCHAR(10) NOT NULL,PID VARCHAR(10),PRIMARY KEY(OID,PID),FOREIGN KEY(OID) REFERENCES PUBLIC.ORDER(OID) ON DELETE CASCADE,FOREIGN KEY(PID) REFERENCES PUBLIC.PRODUCT(PID))
CREATE MEMORY TABLE PUBLIC.PRODUCT_TAGS(PID VARCHAR(10) NOT NULL, TEXT VARCHAR(20), PRIMARY KEY(PID, TEXT), FOREIGN KEY(PID) REFERENCES PUBLIC.PRODUCT(PID) ON DELETE CASCADE)
