CREATE MEMORY TABLE PUBLIC.PRODUCT(PID VARCHAR(10) NOT NULL PRIMARY KEY,NAME VARCHAR(20) NOT NULL,COST DECIMAL(128) NOT NULL,PRICE DECIMAL(128) NOT NULL,QUANTITY BIGINT NOT NULL,SUPPLIER VARCHAR(40) NOT NULL)
